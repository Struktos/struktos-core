{
  "name": "@struktos/core",
  "version": "1.0.0",
  "description": "Enterprise-grade Node.js framework with Hexagonal Architecture, DDD, and CQRS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "hexagonal-architecture",
    "ddd",
    "domain-driven-design",
    "cqrs",
    "unit-of-work",
    "repository-pattern",
    "specification-pattern",
    "dependency-injection",
    "domain-events",
    "event-bus",
    "context-propagation",
    "async-local-storage",
    "enterprise",
    "typescript",
    "framework"
  ],
  "author": "Struktos.js Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/struktos/core.git"
  },
  "bugs": {
    "url": "https://github.com/struktos/core/issues"
  },
  "homepage": "https://struktos.dev",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./domain": {
      "types": "./dist/domain/index.d.ts",
      "require": "./dist/domain/index.js",
      "import": "./dist/domain/index.mjs"
    },
    "./domain/context": {
      "types": "./dist/domain/context/index.d.ts",
      "require": "./dist/domain/context/index.js",
      "import": "./dist/domain/context/index.mjs"
    },
    "./domain/events": {
      "types": "./dist/domain/events/index.d.ts",
      "require": "./dist/domain/events/index.js",
      "import": "./dist/domain/events/index.mjs"
    },
    "./domain/repository": {
      "types": "./dist/domain/repository/index.d.ts",
      "require": "./dist/domain/repository/index.js",
      "import": "./dist/domain/repository/index.mjs"
    },
    "./domain/specification": {
      "types": "./dist/domain/specification/index.d.ts",
      "require": "./dist/domain/specification/index.js",
      "import": "./dist/domain/specification/index.mjs"
    },
    "./application": {
      "types": "./dist/application/index.d.ts",
      "require": "./dist/application/index.js",
      "import": "./dist/application/index.mjs"
    },
    "./application/cqrs": {
      "types": "./dist/application/cqrs/index.d.ts",
      "require": "./dist/application/cqrs/index.js",
      "import": "./dist/application/cqrs/index.mjs"
    },
    "./application/di": {
      "types": "./dist/application/di/index.d.ts",
      "require": "./dist/application/di/index.js",
      "import": "./dist/application/di/index.mjs"
    },
    "./application/host": {
      "types": "./dist/application/host/index.d.ts",
      "require": "./dist/application/host/index.js",
      "import": "./dist/application/host/index.mjs"
    },
    "./infrastructure": {
      "types": "./dist/infrastructure/index.d.ts",
      "require": "./dist/infrastructure/index.js",
      "import": "./dist/infrastructure/index.mjs"
    },
    "./infrastructure/cache": {
      "types": "./dist/infrastructure/cache/index.d.ts",
      "require": "./dist/infrastructure/cache/index.js",
      "import": "./dist/infrastructure/cache/index.mjs"
    },
    "./infrastructure/pipeline": {
      "types": "./dist/infrastructure/pipeline/index.d.ts",
      "require": "./dist/infrastructure/pipeline/index.js",
      "import": "./dist/infrastructure/pipeline/index.mjs"
    },
    "./infrastructure/resilience": {
      "types": "./dist/infrastructure/resilience/index.d.ts",
      "require": "./dist/infrastructure/resilience/index.js",
      "import": "./dist/infrastructure/resilience/index.mjs"
    },
    "./infrastructure/tracing": {
      "types": "./dist/infrastructure/tracing/index.d.ts",
      "require": "./dist/infrastructure/tracing/index.js",
      "import": "./dist/infrastructure/tracing/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.2",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
